{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "react-jsx",
    "lib": ["DOM", "ESNext"],
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true
  }
}